<?xml version="1.0" encoding="UTF-8"?>
<menuDocument>
    <menu>
        <subMenu id="rampage">
            <label>Rampage</label>
            <context>
                <expression>
                    <![CDATA[
from rampage import should_display_rampage_menu

return should_display_rampage_menu(kwargs)
]]>
                </expression>
            </context>
            <scriptItem id="rampage.add_preset">
                <label>Add preset to rampage</label>
                <scriptCode>
                    <![CDATA[
from rampage import add_ramp_preset_menu_callback

add_ramp_preset_menu_callback(kwargs)
]]>
                </scriptCode>
            </scriptItem>
            <scriptItem id="rampage.replace_preset">
                <label>Overrite rampage preset</label>
                <scriptCode>
                    <![CDATA[
from rampage import replace_preset

replace_preset(kwargs)
]]>
                </scriptCode>
            </scriptItem>
            <scriptItem id="rampage.remove_preset">
                <label>Remove rampage preset</label>
                <scriptCode>
                    <![CDATA[
from rampage import remove_preset

remove_preset(kwargs)
]]>
                </scriptCode>
            </scriptItem>
            <subMenu id="rampage.presets">
                <label>Presets</label>
                <scriptMenuStripDynamic id="rampage.preset_strip">
                    <contentsScriptCode>
                        <![CDATA[
from rampage import create_menu_strip

return create_menu_strip(kwargs)
]]>
                    </contentsScriptCode>
                    <scriptCode>
                        <![CDATA[
from rampage import set_ramp_parm_from_chosen_ramp_preset

set_ramp_parm_from_chosen_ramp_preset(kwargs)
]]>
                    </scriptCode>
                </scriptMenuStripDynamic>
            </subMenu>
        </subMenu>
    </menu>
</menuDocument>